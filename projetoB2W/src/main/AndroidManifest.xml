<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.goett.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <protected-broadcast android:name="com.motorola.motocare.INTENT_TRIGGER" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.goett.activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.goett.activitys.PlanetaActivity"
            android:label="@string/title_activity_planeta2"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>